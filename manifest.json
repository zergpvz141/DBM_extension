  {
    "name": "Don't Buy Me!",
    "version": "1.0",
    "description": "",
    "permissions": ["activeTab","tabs","declarativeContent","storage","cookies","https://www-student.cse.buffalo.edu/dontbuyme/*","https://www.amazon.com/*","https://www.barcodespider.com/*"],
    "background": {
      "scripts": ["js/jquery-3.5.1.min.js","background.js"],
      "persistent": false
    },
    "web_accessible_resources": [
      "images/icon_logo.png",
      "images/icon_logoRED.png",
      "images/icon_logoGREEN.png"
    ],
    "content_scripts": [
    {
      "matches": ["https://www.amazon.com/*"],
      "js": ["js/jquery-3.5.1.min.js","amazonPageModifier.js"]
    },
      {
        "matches": ["https://www-student.cse.buffalo.edu/dontbuyme/input.php*"],
        "js": ["js/jquery-3.5.1.min.js","input.js"]
      },
      {
        "js": [ "js/jquery-3.5.1.min.js" ],
        "matches": [ "http://*/*","https://*/*"]
      },
      {
        "js": [ "js/addon/js/bootstrap.min.js" ],
        "matches": [ "http://*/*","https://*/*"]
      }
  ],
    "page_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon_logo16.png",
        "32": "images/icon_logo32.png",
        "48": "images/icon_logo48.png",
        "128": "images/icon_logo128.png"
      }
    },
    "icons": {
      "16": "images/icon_logo16.png",
      "32": "images/icon_logo32.png",
      "48": "images/icon_logo48.png",
      "128": "images/icon_logo128.png"
    },
    "manifest_version": 2
  }
